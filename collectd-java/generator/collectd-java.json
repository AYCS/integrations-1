{
    "conf": {
        "10-jmx.conf": "https://raw.githubusercontent.com/signalfx/integrations/{tag}/collectd-java/10-jmx.conf"
    },
    "default": false,
    "dependencies": [],
    "download_code": false,
    "exclude": false,
    "instructions": [
        {"FROM": "quay.io/signalfuse/collectd"},
        {"MAINTAINER": "SignalFx Support <support+collectd@signalfx.com>"},
        {"ADD": {"managed_config": "/etc/collectd/managed_config"}},
        {"ADD": {"resources/collectd-java/signalfx_types_db":"/usr/share/collectd/java-collectd-plugin/signalfx_types_db"}},
        {"UPGRADE": true},
        {"UPDATE": true},
        {"RUN": "mkdir /usr/share/collectd/java-collectd-plugin"},
        {"RUN": "cp /opt/collectd/collectd-java/signalfx_types_db /usr/share/collectd/java-collectd-plugin/"},
        {"WORKDIR_POST": "/.docker/"},
        {"CMD": "/.docker/run.sh"}
    ],
    "layer": false,
    "resources": [
        {
            "file": "{sep}signalfx_types_db",
            "destination": "{sep}signalfx_types_db"
        }
    ],
    "tags": ["master"]
}